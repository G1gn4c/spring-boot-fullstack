{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "spring-boot-example",
			"image": "gianlucavitto/spring-boot-example:latest",
			"essential": true,
			"memory": 512,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 8080
				}
			],
			"environment": [
				{
					"name": "SPRING_DATASOURCE_URL",
					"value": "jdbc:postgresql://TODO:5432/customer"
				}
			]
		}
	]
}